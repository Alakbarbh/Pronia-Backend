@model Social
@{
    ViewData["Title"] = "Detail";
}

<div class="container my-5">
    <h1> Social detail</h1>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>
                    Image
                </th>
                <th>
                    Link
                </th>
            </tr>
        </thead>

        <tbody>
            <tr>
                <td class="py-1">
                    <i class="fa fa-facebook"></i>
                </td>
                <td class="py-1">
                    @Model.Link
                </td>
            </tr>
        </tbody>
    </table>


    <a class="btn btn-info mt-3" asp-action="Index">Back</a>
</div>



















